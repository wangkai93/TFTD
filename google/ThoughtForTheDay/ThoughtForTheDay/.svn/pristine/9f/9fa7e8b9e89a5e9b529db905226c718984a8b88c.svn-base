<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--  -->
    <style name="WrapContentWidgetStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="FillParentWidgetStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="FillWrapWidgetStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="actionBarTextView" parent="@style/WrapContentWidgetStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="infoTextView" parent="@style/WrapContentWidgetStyle">
        <item name="android:textSize">17sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/white</item>
    </style>
    
    <style name="actionButtonStyle" parent="@style/WrapContentWidgetStyle">
	    <item name="android:layout_centerHorizontal">true</item>
	     <item name="android:orientation">horizontal</item>
	      <item name="android:layout_alignParentTop">true</item>
	       <item name="android:layout_marginTop">2dp</item>
	</style>

    <style name="leftButton" parent="@style/infoTextView">
        <item name="android:background">@drawable/left_button_blue</item>
        <item name="android:layout_marginTop">4dp</item>
    </style>

    <style name="rightButton" parent="@style/WrapContentWidgetStyle">
        <item name="android:textSize">17sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:background">@drawable/right_button_white</item>
    </style>

    <style name="WrapContentWidgetStyleTab">
        <item name="android:layout_width">180dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="infoTextView2" parent="@style/infoTextView">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="infoView" parent="@style/FillWrapWidgetStyle">
        <item name="android:textSize">16sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/gray</item>
    </style>

    <style name="titleTextView" parent="@style/FillWrapWidgetStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">24sp</item>
        <item name="android:singleLine">true</item>
    </style>
    
    <style name="readTextView" parent="@style/WrapContentWidgetStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="favoriteTextView">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">24sp</item>
        <item name="android:singleLine">true</item>
    </style>

